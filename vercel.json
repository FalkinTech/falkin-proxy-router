{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.falkin.com" }],
      "destination": "https://falkin.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/check/:path*",
      "has": [{ "type": "host", "value": "falkin.com" }],
      "destination": "https://microsite-seven.vercel.app/:path*"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "falkin.com" }],
      "destination": "https://able-ways-807681.framer.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=31536000" },
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
